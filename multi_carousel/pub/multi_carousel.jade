      hr.featurette-divider
      // /END THE FEATURETTES
      // FOOTER
      footer
        p.pull-right
          a(href='#') Back to top
        p
          | &copy; 2013 HonSys&period;com and Eviz&period;biz &middot;
          a(href='#') Privacy
          | &middot;
          a(href='#') Terms
