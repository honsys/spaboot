html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='description', content='multiple carousels')
    meta(name='author', content='david b. hon')
    link(rel='shortcut icon', href='/img/favicon.png')
    title Multiple Carousels via Bootstrap
    // Bootstrap core CSS
    link(href='/css/bootstrap.css', rel='stylesheet')
    // HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries
    //if lt IE 9
      script(src='//oss.maxcdn.com/libs/html5shiv/3.6.2/html5shiv.js')
      script(src='../../docs-assets/js/respond.min.js')
    // Custom styles for this template
    link(href='carousel.css', rel='stylesheet')
  // NAVBAR ==================================================
  body
    .navbar-wrapper
      .container
        .navbar.navbar-inverse.navbar-static-top
          .container
            .navbar-header
              button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
                span.icon-bar
                span.icon-bar
                span.icon-bar
              a.navbar-brand(href='#') Project name
            .navbar-collapse.collapse
              ul.nav.navbar-nav
                li.active
                  a(href='#') Home
                li
                  a(href='#about') About
                li
                  a(href='#contact') Contact
                li.dropdown
                  a.dropdown-toggle(href='#', data-toggle='dropdown')
                    | Dropdown
                    b.caret
                  ul.dropdown-menu
                    li
                      a(href='#') 1st Action
                    li
                      a(href='#') 2nd Action
                    li
                      a(href='#') 3rd Action
                    li.divider
                    li.dropdown-header Nav dropdown header
                    li
                      a(href='#') Separated link
                    li
                      a(href='#') One more separated link
    // topCarousel ==================================================
    #topCarousel.carousel.slide(data-ride='carousel')
      // Indicators
      ol.carousel-indicators
        li.active(data-target='#topCarousel', data-slide-to='0')
        li(data-target='#topCarousel', data-slide-to='1')
        li(data-target='#topCarousel', data-slide-to='2')
      .carousel-inner
        .item.active
          img(src='data:image/png;base64,', data-src='holder.js/900x500/auto/#777:#7a7a7a/text:First slide', alt='First slide')
          .container
            .carousel-caption
              h1 1st Slide of topCarousel
              p 1st paragraph of 1st slide of topCarousel&period;
              p
                a.btn.btn-lg.btn-primary(href='#', role='button') 1st button anchor of 1st slide of topCarousel
        .item
          img(src='data:image/png;base64,', data-src='holder.js/900x500/auto/#666:#6a6a6a/text:Second slide', alt='Second slide')
          .container
            .carousel-caption
              h1 2nd Slide of topCarousel
              p 1st paragraph of 2nd slide of topCarousel&period;
              p
                a.btn.btn-lg.btn-primary(href='#', role='button') 1st button anchor of 2nd slide of topCarousel
        .item
          img(src='data:image/png;base64,', data-src='holder.js/900x500/auto/#555:#5a5a5a/text:Third slide', alt='Third slide')
          .container
            .carousel-caption
              h1 3rd Slide of topCarousel
              p 1st paragraph of 2rd slide of topCarousel&period;
              p
                a.btn.btn-lg.btn-primary(href='#', role='button') 1st button anchor of 3rd slide of topCarousel
      a.left.carousel-control(href='#topCarousel', data-slide='prev')
        span.glyphicon.glyphicon-chevron-left
      a.right.carousel-control(href='#topCarousel', data-slide='next')
        span.glyphicon.glyphicon-chevron-right
    // /.topcarousel
    // botmCarousel ==================================================
