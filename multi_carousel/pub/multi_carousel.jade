    // Marketing messaging and featurettes ==================================================
    // Wrap the rest of the page in another container to center all the content.
    .container.marketing
      // Three columns of text below the carousel
      .row
        .col-lg-4
          img.img-circle(src='data:image/png;base64,', data-src='holder.js/140x140', alt='Generic placeholder image')
          h2 1st Column Heading
          p 1st paragraph of 1st column&period;
          p
            a.btn.btn-default(href='#', role='button') View details of 1st column &raquo;
        // /.col-lg-4
        .col-lg-4
          img.img-circle(src='data:image/png;base64,', data-src='holder.js/140x140', alt='Generic placeholder image')
          h2 2nd Column Heading
          p 1st paragraph of 2nd column&period;
          p
            a.btn.btn-default(href='#', role='button') View details of 2nd column &raquo;
        // /.col-lg-4
        .col-lg-4
          img.img-circle(src='data:image/png;base64,', data-src='holder.js/140x140', alt='Generic placeholder image')
          h2 3rd Column Heading
          p 1st paragraph of 3rd column&period;
          p
            a.btn.btn-default(href='#', role='button') View details of 3rd column &raquo;
